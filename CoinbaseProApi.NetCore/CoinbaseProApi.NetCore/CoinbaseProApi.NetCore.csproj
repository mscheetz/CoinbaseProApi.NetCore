<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Matt Scheetz</Authors>
    <Description>Api library for accessing the CoinbasePro exchange Api built on .Net Standard</Description>
    <Copyright>2018 Matt Scheetz</Copyright>
    <PackageProjectUrl>https://github.com/mscheetz/CoinbaseProApi.NetCore</PackageProjectUrl>
    <RepositoryUrl>https://github.com/mscheetz/CoinbaseProApi.NetCore</RepositoryUrl>
    <PackageIconUrl></PackageIconUrl>
    <PackageLicenseUrl>https://github.com/mscheetz/CoinbaseProApi.NetCore/blob/master/LICENSE</PackageLicenseUrl>
    <PackageTags>coinbase bitcoin ethereum litecoin cryptocurrency exchange trading api net standard</PackageTags>
    <PackageReleaseNotes>Include comments for nuget package</PackageReleaseNotes>
    <IsPackable>true</IsPackable>
    <Version>1.1.1</Version>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DocumentationFile>C:\Source\Git\CoinbaseProApi.NetCore\CoinbaseProApi.NetCore\CoinbaseProApi.NetCore\CoinbaseProApi.NetCore.xml</DocumentationFile>
    <OutputPath>bin\Release\</OutputPath>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="DateTimeHelpers" Version="1.3.0" />
    <PackageReference Include="FileRepository" Version="1.1.0" />
    <PackageReference Include="Newtonsoft.Json" Version="11.0.2" />
    <PackageReference Include="RESTApiAccess" Version="1.2.1" />
  </ItemGroup>

</Project>
