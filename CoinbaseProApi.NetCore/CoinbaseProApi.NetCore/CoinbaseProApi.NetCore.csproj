<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <Authors>Matt Scheetz</Authors>
    <Description>Api library for accessing the CoinbasePro exchange Api</Description>
    <Copyright>2018 Matt Scheetz</Copyright>
    <PackageProjectUrl>https://github.com/mscheetz/CoinbaseProApi.NetCore</PackageProjectUrl>
    <RepositoryUrl>https://github.com/mscheetz/CoinbaseProApi.NetCore</RepositoryUrl>
    <PackageIconUrl></PackageIconUrl>
    <PackageLicenseUrl>https://github.com/mscheetz/CoinbaseProApi.NetCore/blob/master/LICENSE</PackageLicenseUrl>
    <PackageTags>coinbase bitcoin ethereum litecoin cryptocurrency exchange trading api net core</PackageTags>
    <PackageReleaseNotes>Api library for accessing the CoinbasePro exchange Api</PackageReleaseNotes>
    <IsPackable>true</IsPackable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="DateTimeHelpers" Version="1.0.0" />
    <PackageReference Include="FileRepository" Version="1.0.0" />
    <PackageReference Include="Newtonsoft.Json" Version="11.0.2" />
    <PackageReference Include="RESTApiAccess" Version="1.0.0" />
    <PackageReference Include="xunit" Version="2.4.0" />
    <PackageReference Include="xunit.runner.visualstudio" Version="2.4.0" />
  </ItemGroup>

</Project>
